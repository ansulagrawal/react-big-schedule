{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "plugins": ["react"],
  "parser": "@babel/eslint-parser",
  "extends": ["airbnb"],
  "parserOptions": { "ecmaVersion": "latest", "sourceType": "module" },
  "rules": {
    "max-len": ["error", { "code": 200 }],
    "arrow-parens": ["error", "as-needed"],
    "linebreak-style": "off",
    "object-curly-newline":"off"
  }
}
